{
    "name": "jsiterable",
    "version": "2.0.0",
    "description": "No-dependency javascript iterable library exposing functional operations over iterables",
    "main": "./lib/index.js",
    "scripts": {
        "clean": "rimraf ./lib && rimraf ./coverage",
        "coverage": "npm run test && opener ./coverage/index.html",
        "build": "tsc",
        "test": "nyc mocha",
        "prepublishOnly": "npm run clean && npm run build && npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/riteshrao/jsiterable.git"
    },
    "keywords": [
        "Iterables",
        "Functional"
    ],
    "author": "Ritesh Rao",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/riteshrao/jsiterable/issues"
    },
    "homepage": "https://github.com/riteshrao/jsiterable#readme",
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.6",
        "chai": "^4.2.0",
        "mocha": "^6.1.4",
        "nyc": "^14.1.1",
        "opener": "^1.5.1",
        "rimraf": "^2.6.3",
        "source-map-support": "^0.5.11",
        "ts-node": "^8.0.3",
        "tslint": "^5.14.0",
        "tslint-no-unused-expression-chai": "^0.1.4",
        "typescript": "^3.3.3333"
    },
    "nyc": {
        "all": true,
        "cache": false,
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "**/*.d.ts",
            "test/**",
            "src/types.ts"
        ],
        "reporter": [
            "html",
            "json",
            "text-summary"
        ],
        "extension": [
            ".ts"
        ],
        "source-map": true,
        "instrument": true
    }
}
